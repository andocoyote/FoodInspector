{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "KeyVault": {
    "VaultName": "kv-food-inspector"
  },
  "CosmosDb": {
    "Database": "FoodInspector",
    "Containers": {
      "InspectionData": "InspectionRecordsAggregated"
    }
  },
  "Storage": {
    "StorageAccountName": "stfoodinspector"
  },
  "AzureAI": {
    "Endpoint": "https://ai-services-food-inspector.openai.azure.com/",
    "Deployment": "gpt-4o-mini",
    "Question": "This it the inspection data to analyze: ",
    "SystemMessageFileName": "SystemMessage-FoodInspector.txt",
    "JsonSchemaFileName": "SystemMessageSchema-FoodInspector.json",
    "SystemMessageBlobContainer": "system-messages"
  }
}